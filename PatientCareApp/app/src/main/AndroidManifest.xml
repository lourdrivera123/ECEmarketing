<?xml version="1.0" encoding="utf-8"?><!--suppress ALL -->
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zem.patientcareapp">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <!-- for card.io card scanning -->
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />
    <uses-feature
        android:name="android.hardware.camera.autofocus"
        android:required="false" />

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@drawable/ic_app"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            launchMode="singleInstance"
            android:theme="@android:style/Theme.Holo.Light"
            android:configChanges="keyboardHidden|orientation|screenSize">

        </activity>
        <activity
            android:name=".SignUpFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".AccountFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".ContactsFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".ListOfDoctorsFragment"
            android:configChanges="keyboardHidden|orientation|screenSize"></activity>
        <activity
            android:name=".DoctorActivity"
            android:theme="@style/Theme.MyFancyTheme"
            launchMode="singleInstance"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".PatientProfileFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".PatientHistoryFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".ShoppingCartFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".ProductsFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".HomeTileActivity"
            android:theme="@android:style/Theme.Holo.Light"
            launchMode="singleInstance"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".MasterTabActivity"
            android:theme="@android:style/Theme.Holo.Light"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".EditTabsActivity"
            android:theme="@android:style/Theme.Holo.Light"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <activity
            android:name=".PatientMedicalRecordActivity"
            launchMode="singleInstance"
            android:theme="@android:style/Theme.Holo.Light" />
        <activity
            android:name=".SelectedProductActivity"
            android:theme="@style/Theme.MyFancyTheme" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".PrescriptionFragment"
            android:configChanges="keyboardHidden|orientation|screenSize" />

        <activity android:name=".Patient" />
        <activity android:name=".Medicine" />
        <activity android:name=".Doctor" />
        <activity android:name=".PatientRecord" />
        <activity android:name=".RoundImage" />
        <activity android:name=".FileCache" />
        <activity android:name=".Utils" />
        <activity android:name=".MemoryCache" />
        <activity android:name=".LazyAdapter" />
        <activity android:name=".ImageLoader" />
        <activity android:name=".DbHelper" />
        <activity android:name=".PatientConsultationActivity" />
        <activity android:name=".PatientConsultationFragment" />
        <activity android:name=".AppController" />
        <activity android:name=".LruBitmapCache" />
        <activity android:name=".Sync" />
        <activity android:name=".ProductCategory" />
        <activity android:name=".ProductSubCategory" />
        <activity android:name=".Product" />
        <activity android:name=".ProductCategoriesFragment" />
        <activity android:name=".PreloadNonUserRelatedData" />
        <activity android:name=".ServerRequest" />
        <activity android:name=".Basket" />
        <activity android:name=".PromoDiscount" />
        <activity android:name=".PromoFreeProducts" />
        <activity android:name=".ImageItem" />
        <meta-data
            android:name="android.app.default_searchable"
            android:value=".SearchableActivity" />
        <activity
            android:label="@string/app_name"
            android:launchMode="singleTop"
            android:name=".SearchableActivity" >
            <intent-filter >
                <action android:name="android.intent.action.SEARCH" />
            </intent-filter>
            <intent-filter >
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>

        <activity android:name=".samplepaypal" />

        <service
            android:name="com.paypal.android.sdk.payments.PayPalService"
            android:exported="false" />

        <activity android:name="com.paypal.android.sdk.payments.PaymentActivity" />
        <activity android:name="com.paypal.android.sdk.payments.LoginActivity" />
        <activity android:name="com.paypal.android.sdk.payments.PaymentMethodActivity" />
        <activity android:name="com.paypal.android.sdk.payments.PaymentConfirmActivity" />
        <activity
            android:name="io.card.payment.CardIOActivity"
            android:configChanges="keyboardHidden|orientation" />
        <activity android:name="io.card.payment.DataEntryActivity" />

    </application>
</manifest>
